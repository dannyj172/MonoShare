{
  "compilerOptions": {
    // "module": "node16",
    // "moduleResolution": "node16",
    "module": "commonjs",
    "moduleResolution": "node",
    "ignoreDeprecations": "6.0",
    "strict": true,
    "outDir": "./built",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "types": ["node"]
  },
  "include": [
    "src/**/*",
    "src/config/prisma.config.ts",
    "src/prisma",
    "prisma/seed.ts"
  ]
}
